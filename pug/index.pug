- var testing = "true"

doctype html
html(lang="en")
  head
    title Test App
    link(href="https://fonts.googleapis.com/css?family=Catamaran:400,700|Libre+Baskerville:700" rel="stylesheet")
    link(href="css/wigc.css" rel="stylesheet")
    //meta(name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1.0, user-scalable=no")
    //meta(name="viewport" content="width=device-width, initial-scale=1")
    meta(name="viewport" content="user-scalable=no, width=device-width, initial-scale=1, maximum-scale=1")
    meta(name="format-detection" content="telephone=no")
    //
  body
    main#app
    
      section.errors(v-if="error_msg" v-html="error_msg")
      
      .video-holder
        //- video(src="video/glory_bands.mp4" autoplay muted loop)
        
        include partials/_header
        include partials/_sidebar.pug
        include partials/_footer.pug
            
      include partials/sections/_my.pug
      include partials/sections/_sessions.pug
      include partials/sections/_vendors.pug
      include partials/sections/_map.pug
      include partials/sections/_meetoo.pug
            
    .javascripts
      if testing
        script(src="https://code.jquery.com/jquery-3.1.1.min.js")
        script(src="https://unpkg.com/vue")
        script(src="http://hammerjs.github.io/dist/hammer.min.js")
      else 
        script(src="cordova.js")
        script(src="js/vendor/jquery-3.2.1.min.js")
        script(src="js/vendor/vue.min.js")
        script(src="js/vendor/hammer.min.js")
      script(src="js/wigc.js")
      script(src="js/pinch-zoom.js")